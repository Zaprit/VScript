#Object class for VScript
Object.define(){
 alias "$1"=Object
 export funcs$1="${@: 2}"
}

new(){
 for func in funcs$1
 do
  alias "$func.${1#*:}"=$func
 done
}

Object(){
 if [ "$2" != "=" ]
 then
  throw "Error In $: = expected"
 fi
 if [ "$3" == "new" ]
 then
  export $1=$(new $1 $4 $5 $6 $7)
 else
  export $1=$($3 $4 $5 $6)
 fi
}


destroy(){

}
